{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/swiper-slider",
	"version": "0.1.0",
	"title": "Swiper Slider",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"supports": {
		"html": false
	},
	"textdomain": "swiper-slider",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"slides": {
			"type": "array",
			"default": [
				{
					"heading": {
						"type": "array",
						"source": "children"
					},
					"imgLink": {
						"type": "array",
						"source": "children"
					},
					"link": {
						"type": "string",
						"source": "children"
					},
					"rating": {
						"type": "string",
						"source": "children"
					}
				}
			]
		}
    }
}
